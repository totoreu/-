#include <string>
#include <vector>

using namespace std;

// https://school.programmers.co.kr/learn/courses/30/lessons/120913

vector<string> solution(string my_str, int n) {
    vector<string> answer;
    for(int i = 0; i < my_str.size(); i+=n)
        answer.push_back(my_str.substr(i, n));
    return answer;
}
